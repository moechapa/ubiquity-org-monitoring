        <isEstimatedWaitTimeEnabled>true</isEstimatedWaitTimeEnabled>
        <isFallbackMessageEnabled>false</isFallbackMessageEnabled>
        <isSaveTranscriptEnabled>false</isSaveTranscriptEnabled>
