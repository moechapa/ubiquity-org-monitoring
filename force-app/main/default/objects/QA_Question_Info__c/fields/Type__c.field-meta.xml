    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Standard</fullName>
                <default>true</default>
                <label>Standard</label>
            </value>
            <value>
                <fullName>Checkbox</fullName>
                <default>false</default>
                <label>Checkbox</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
