<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Request_Submission_Before_Advisor_Added</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Fund_List_Status__c, &quot;Request Submitted&quot;),
(Advisor_Added_in_Paradigm__c = False))</errorConditionFormula>
    <errorMessage>Advisor must be added in Paradigm for Ops to build the fund list.</errorMessage>
</ValidationRule>
