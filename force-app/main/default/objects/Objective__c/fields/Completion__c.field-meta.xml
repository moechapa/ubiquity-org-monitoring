<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completion__c</fullName>
    <description>Sum of all KR percentages divided by the total number of Key Results.</description>
    <formula>IF(X_of_Key_Results__c&gt;0, KR_Percent_Sum__c / X_of_Key_Results__c,  Completion_per__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Sum of all KR percentages divided by the total number of Key Results.</inlineHelpText>
    <label>Completion</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
