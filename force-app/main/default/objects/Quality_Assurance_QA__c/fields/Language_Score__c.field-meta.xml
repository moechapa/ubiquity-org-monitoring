<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Language_Score__c</fullName>
    <description>10 points for using professional and polite language, 0 points if not.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL( Language__c, &quot;Yes&quot;)||ISPICKVAL( Language__c, &quot;N/A&quot;),10,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>10 points for using professional and polite language, 0 points if not.</inlineHelpText>
    <label>Language Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
