<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Value_Added_Score__c</fullName>
    <description>5 Points if value added in reply, 0 points if not.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL( Value_Added__c, &quot;Yes&quot;)||ISPICKVAL( Value_Added__c, &quot;N/A&quot;),5,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>5 Points if value added in reply, 0 points if not.</inlineHelpText>
    <label>Value Added Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
