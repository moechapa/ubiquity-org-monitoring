<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Additional_Assistance_Score__c</fullName>
    <description>5 Points if additional assistance is offered, 0 points if not.</description>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL( Additional_Assistance__c, &quot;Yes&quot;)||ISPICKVAL( Additional_Assistance__c, &quot;N/A&quot;),5,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>5 Points if additional assistance is offered, 0 points if not.</inlineHelpText>
    <label>Additional Assistance Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
