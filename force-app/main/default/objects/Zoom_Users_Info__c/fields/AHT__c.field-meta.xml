<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AHT__c</fullName>
    <description>Average handle time of inbound and outbound calls in minutes.</description>
    <externalId>false</externalId>
    <formula>IF(Inbound_Calls_Duration__c + Outbound_Calls_Duration__c &gt; 0,
((Inbound_Calls_Duration__c + Outbound_Calls_Duration__c)/60) / (  Inbound_Calls__c  +  Outbound_Calls__c ),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Average handle time of inbound and outbound calls in minutes.</inlineHelpText>
    <label>AHT</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
