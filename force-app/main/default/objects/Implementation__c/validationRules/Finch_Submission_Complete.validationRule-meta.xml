<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Finch_Submission_Complete</fullName>
    <active>true</active>
    <errorConditionFormula>Finch_Implementation__c = TRUE
&amp;&amp;
ISCHANGED(Implementation_Status__c) 
&amp;&amp;
ISPICKVAL(Implementation_Status__c, &quot;Completed&quot;)
&amp;&amp;
(Plan__r.Most_Recent_Payroll_Contribution_Total__c &lt; 1 || ISNULL(Plan__r.Most_Recent_Payroll_Contribution_Total__c))</errorConditionFormula>
    <errorMessage>There was a problem with the Finch submission. The implementation cannot complete if the Most Recent Contribution Total on the Plan record cannot be 0 or blank.</errorMessage>
</ValidationRule>
