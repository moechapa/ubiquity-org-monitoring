<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_After_End</fullName>
    <active>true</active>
    <description>Start date cannot be after end date</description>
    <errorConditionFormula>Start_Date__c &gt; End_Date__c</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>The promotion cannot end before it&apos;s started.</errorMessage>
</ValidationRule>
