<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Calc_Type_Missing</fullName>
    <active>true</active>
    <description>If &quot;Other&quot; selected on Calculation Type, must enter Other description</description>
    <errorConditionFormula>AND
(INCLUDES( Calculation_Type__c , &quot;Other&quot;), 

 ISBLANK( Other_Calculation_Type__c ))</errorConditionFormula>
    <errorDisplayField>Other_Calculation_Type__c</errorDisplayField>
    <errorMessage>Confirm &quot;Other&quot; Calculation type</errorMessage>
</ValidationRule>
