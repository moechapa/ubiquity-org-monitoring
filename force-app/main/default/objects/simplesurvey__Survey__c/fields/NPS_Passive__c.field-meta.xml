<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NPS_Passive__c</fullName>
    <description>Flags NPS Survey as Passive for use with overall NPS Calc - https://www.getfeedback.com/resources/salesforce/measure-nps-salesforce/</description>
    <externalId>false</externalId>
    <formula>IF(AND

(VALUE(TEXT( NPS__c )) &gt; 6 ,  (VALUE(TEXT(NPS__c ))) &lt; 9), 1, 0)</formula>
    <inlineHelpText>1 = Promoter 0 = Not Passive</inlineHelpText>
    <label>NPS Passive</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
