<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Required</fullName>
    <active>true</active>
    <errorConditionFormula>ISNULL(AccountId)</errorConditionFormula>
    <errorDisplayField>AccountId</errorDisplayField>
