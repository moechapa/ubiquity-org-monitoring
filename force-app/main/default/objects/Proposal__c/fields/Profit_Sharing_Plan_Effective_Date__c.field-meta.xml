<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Profit_Sharing_Plan_Effective_Date__c</fullName>
    <description>Prior Year Profit Sharing Plan Effective Date</description>
    <formula>IF(
    ISPICKVAL(Prior_Year_Profit_Sharing_Contribution__c, &quot;Yes&quot;),
    DATE(YEAR(TODAY()) - 1, 1, 1),
    NULL
)</formula>
    <label>Profit Sharing Plan Effective Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
