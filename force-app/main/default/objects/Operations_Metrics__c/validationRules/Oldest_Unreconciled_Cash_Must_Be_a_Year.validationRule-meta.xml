<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Oldest_Unreconciled_Cash_Must_Be_a_Year</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( Oldest_Unreconciled_Cash__c ) != 4 &amp;&amp; NOT(ISBLANK(Oldest_Unreconciled_Cash__c ))</errorConditionFormula>
    <errorMessage>Please enter a 4 digit year, ex: 2023</errorMessage>
</ValidationRule>
