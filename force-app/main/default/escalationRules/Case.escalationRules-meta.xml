<?xml version="1.0" encoding="UTF-8"?>
<EscalationRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <escalationRule>
        <fullName>Approvals for PD Enhancement Requests</fullName>
        <active>false</active>
        <ruleEntry>
            <businessHoursSource>Case</businessHoursSource>
            <criteriaItems>
                <field>Case.RecordTypeId</field>
                <operation>equals</operation>
                <value>Marketing Request,PD Enhancement Request</value>
            </criteriaItems>
            <criteriaItems>
                <field>Case.Department__c</field>
                <operation>equals</operation>
                <value>Marketing,CRG,Sales,Partnerships</value>
            </criteriaItems>
            <disableEscalationWhenModified>true</disableEscalationWhenModified>
            <escalationAction>
                <minutesToEscalation>30</minutesToEscalation>
                <notifyCaseOwner>false</notifyCaseOwner>
                <notifyEmail>jkompor@theonline401k.com</notifyEmail>
                <notifyEmail>jbesse@theonline401k.com</notifyEmail>
                <notifyEmail>mhughes@theonline401k.com</notifyEmail>
                <notifyToTemplate>AutomaticEmails/SFEnhancementRequestApproval</notifyToTemplate>
            </escalationAction>
            <escalationStartTime>CaseCreation</escalationStartTime>
        </ruleEntry>
    </escalationRule>
