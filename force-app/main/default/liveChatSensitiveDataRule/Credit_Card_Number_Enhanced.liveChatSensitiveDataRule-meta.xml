<?xml version="1.0" encoding="UTF-8"?>
<LiveChatSensitiveDataRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionType>Replace</actionType>
    <enforceOn>2</enforceOn>
    <isEnabled>false</isEnabled>
    <pattern>^(?:\d[ -]*?){13,19}$</pattern>
    <priority>0</priority>
    <processingType>RealTime</processingType>
    <replacement>[removed CC number]</replacement>
    <version>TWO</version>
</LiveChatSensitiveDataRule>
