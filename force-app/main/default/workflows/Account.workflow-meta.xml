    <rules>
        <fullName>Account - Update - If Industry %3D MG Trust</fullName>
        <actions>
            <name>Copy_Matrix_to_Industry_field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.Custodian__c</field>
            <operation>equals</operation>
            <value>&quot;MG Trust Company, LLC.&quot;</value>
