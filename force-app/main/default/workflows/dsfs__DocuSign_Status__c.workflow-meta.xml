    <rules>
        <fullName>Paradigm Docusign Created</fullName>
        <actions>
            <name>New_Paradigm_Docusign_Sent</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>dsfs__DocuSign_Status__c.dsfs__Subject__c</field>
            <operation>startsWith</operation>
            <value>Your Plan&apos;s</value>
        </criteriaItems>
        <description>Triggers when new envelope status is created due to document creation in Paradigm.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
