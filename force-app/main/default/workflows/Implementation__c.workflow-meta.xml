    <rules>
        <fullName>Implementation - Email - Implementation Complete</fullName>
        <actions>
            <name>Implementation_Completed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Implementation__c.Implementation_Completion_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Alerts BI Team when Implementation is marked complete</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Implementation - Email - New Implementation</fullName>
        <actions>
            <name>New_Implementation_Created</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Implementation__c.Product_Type__c</field>
            <operation>equals</operation>
            <value>401(k)</value>
        </criteriaItems>
        <description>Alerts New Plan when an Opportunity is Won and an Implementation is opened.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Implementation - Email - New Schwab Sale</fullName>
        <actions>
            <name>Alert_Partnerships_for_Schwab_Fee_Disclosur2e</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Implementation__c.Custodian__c</field>
            <operation>equals</operation>
            <value>&quot;Schwab &amp; Co., Inc.&quot;</value>
        </criteriaItems>
        <description>Alerts Partnerships Support when a Schwab plan is sold to generate the fee disclosure.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Implementation - Email - Plan Stall</fullName>
        <actions>
            <name>Notify_RPC_s</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Implementation__c.Implementation_Status__c</field>
            <operation>equals</operation>
            <value>Plan Stalled</value>
        </criteriaItems>
        <criteriaItems>
            <field>Implementation__c.X6_Month_Sale__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Notifies RPCs of plan stall</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Implementation - Email - Unresponsive</fullName>
        <actions>
            <name>Notify_RPC_unresponsive</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Implementation__c.Unresponsive__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Notifies RPCs of potential plan stall and unresponsive plan</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
