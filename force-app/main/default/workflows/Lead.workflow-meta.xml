    <rules>
        <fullName>Lead - Email - Advisor Lead 3%2B Hours</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
            <value>Assigned</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.n2de__Time_to_action_mins__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.OwnerId</field>
            <operation>contains</operation>
            <value>Partnership</value>
        </criteriaItems>
        <description>Alerts Chris Jasinski when an Advisor lead has not been responded to in 3 or more hours.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Alert_Chris_When_Advisor_Lead_Not_Answered_3_Hours</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Lead.n2de__Last_distributed__c</offsetFromField>
            <timeLength>3</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Lead - Task - First Call for Sales</fullName>
        <actions>
            <name>Call_New</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <description>1st Task for new lead assigned to Sales</description>
        <formula>ISCHANGED(Status)  &amp;&amp;  ISPICKVAL(Status, &apos;Assigned&apos;)  &amp;&amp;  NOT(ISPICKVAL(Business_Unit__c, &apos;Simply Retirement&apos;))  &amp;&amp; NOT(ISPICKVAL(LeadSource, &apos;Content Syndication&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
