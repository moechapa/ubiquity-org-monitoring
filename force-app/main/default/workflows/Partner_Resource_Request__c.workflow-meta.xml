<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Partner_Resource_Request</fullName>
        <description>Partner Resource Request</description>
        <protected>false</protected>
        <recipients>
            <recipient>hclaxton@theonline401k.com</recipient>
            <type>user</type>
        </recipients>
        <recipients>
            <field>Project_Owner__c</field>
            <type>userLookup</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>Ubiquity_PGST/Partner_Resource_Request</template>
    </alerts>
    <rules>
        <fullName>Partner Resource Request</fullName>
        <actions>
            <name>Partner_Resource_Request</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Partner_Resource_Request__c.Project_Owner__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Alert project owner when partner resource request is created.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
