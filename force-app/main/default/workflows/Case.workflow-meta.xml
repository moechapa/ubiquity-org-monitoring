    <rules>
        <fullName>Excess Cash</fullName>
        <actions>
            <name>Excess_Cash_Email</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.OwnerId</field>
            <operation>equals</operation>
            <value>Payroll</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Excess_Cash_Amount__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <description>Triggers Excess Cash email when an unknown deposit case is created.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
