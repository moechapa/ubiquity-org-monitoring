    <rules>
        <fullName>Implementations New to Assigned</fullName>
        <actions>
            <name>Status_to_Assigned</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Change status from New to Assigned when Implementations team member takes ownership from their Queue</description>
        <formula>AND( ISCHANGED(OwnerId), PRIORVALUE(OwnerId) = &quot;00G37000000PDff&quot;, NOT(BEGINS(OwnerId, &quot;00G&quot;)) )</formula>
        <triggerType>onAllChanges</triggerType>
