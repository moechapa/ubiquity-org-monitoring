    <rules>
        <fullName>Aged Payroll</fullName>
        <actions>
            <name>Aged_Payroll</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.OwnerId</field>
            <operation>equals</operation>
            <value>Payroll</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Aged_Payroll_Amount__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <description>Triggers Aged Payroll email when an unknown deposit case is created.   Deactivated per office hours with Sonia, Patti and Brian 2.25.2021</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Case - Email - Child Case Resolved</fullName>
        <actions>
            <name>Email_to_Child_Case_Creator_Resolved</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.ParentId</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Resolved,Duplicate</value>
        </criteriaItems>
        <description>Email Parent Owner with Case # and Account Name with notice that Child Case is Resolved</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Case - Email - List Source Request</fullName>
        <actions>
            <name>Email_List_Sourcing_Request_Notification</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.ParentId</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Subject</field>
            <operation>startsWith</operation>
            <value>List Sourcing Request</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.OwnerId</field>
            <operation>notContain</operation>
            <value>Payroll,Compliance,Distributions,Operations,Finance,Transitions</value>
        </criteriaItems>
        <description>Email Child Case Owner to Review List Source Request</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Case - Upadate - TTA %3D TTC</fullName>
        <actions>
            <name>Update_Time_to_Action</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Resolved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.n2de__Time_to_action_mins__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>If case is Closed and Time to Action is Blank use time to close as Time to action</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Case - Update - Entitlement start %3D Now for Child Case</fullName>
        <actions>
            <name>Adjust_Entitlement_Process_Start_Time</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.ParentId</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Resets Milestone / Entitlement start time to Creation date/time of Child Case (otherwise it would copy the earlier start time of the parent)</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Case - Update - Reopened Count %2B 1</fullName>
        <actions>
            <name>Reopened_Count_1</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Adds +1 to &quot;Reopened Count&quot; in a case when the status changes from any &quot;CLOSED&quot; status to an &quot;OPEN&quot; status.  Counts # of times a case is reopened.</description>
        <formula>AND(IsClosed = FALSE, PRIORVALUE(IsClosed) = TRUE)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Case - Update - Trucate Description</fullName>
        <actions>
            <name>Truncate_Case_Description</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>contains</operation>
            <value>inbox</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.CreatedById</field>
            <operation>notEqual</operation>
            <value>Automation Team</value>
        </criteriaItems>
        <description>Truncated Description field to first 300 characters</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>CaseResCSAT</fullName>
        <active>false</active>
        <description>Send Case Res CSAT email when Case = Resolved and Last Survey Date &gt; 60 Days</description>
        <formula>NOT(CONTAINS(CreatedBy.Username,&quot;automationteam&quot;))  &amp;&amp;  AND ( ISPICKVAL( Status , &quot;Resolved&quot;),  EvenOdd__c = &quot;Even&quot;,  OR( (Case_Team__c  =&quot;Client Support&quot;) , (Case_Team__c  =&quot;West Coast Servicing&quot;) ,  (Case_Team__c  =&quot;East Coast Servicing&quot; ) , (Case_Team__c  =&quot;Mountain/Central Servicing&quot;) , (Case_Team__c  =&quot;Platinum Servicing&quot;) , (Case_Team__c  =&quot;Payroll&quot; ),(Case_Team__c  =&quot;Billing&quot; ),(Case_Team__c  =&quot;Saver Success&quot; ), (Case_Team__c  =&quot;Single(k)&quot;), (Case_Team__c  =&quot;Compliance&quot; )),  (ISBLANK( ParentId )),   NOT(ISBLANK(  Contact.Id  )),    NOT( CONTAINS( Contact.Email , &quot;myubiquity.com&quot;)),  (NOT( CONTAINS( Contact.Email , &quot;principal.com&quot;) &amp;&amp; NOT( ISPICKVAL(  Contact.Role__c , &quot;Advisor&quot;)))),   (Contact.Survey_Opt_Out__c = false),  OR  ( ISBLANK(Contact.Last_Created_Survey_Date__c),  (TODAY () - (Contact.Last_Created_Survey_Date__c)) &gt; 60), BEGINS (OwnerId, &apos;005&apos;), DATEVALUE(ClosedDate) = TODAY ())</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>CaseResCsat_15_min_delay</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Case.Closed_Date_Delay__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>CaseResCSAT%2BNPS</fullName>
        <active>false</active>
        <description>Send Case Res CSAT+NPS  email when Case = Resolved and Last Survey Date &gt; 60 Days</description>
        <formula>NOT(CONTAINS(CreatedBy.Username,&quot;automationteam&quot;))  &amp;&amp;  AND ( ISPICKVAL( Status , &quot;Resolved&quot;),  EvenOdd__c = &quot;Odd&quot;,  OR( (Case_Team__c  =&quot;Client Support&quot;) , (Case_Team__c  =&quot;West Coast Servicing&quot;) ,  (Case_Team__c  =&quot;East Coast Servicing&quot; ) , (Case_Team__c  =&quot;Mountain/Central Servicing&quot;) , (Case_Team__c  =&quot;Platinum Servicing&quot;) , (Case_Team__c  =&quot;Payroll&quot; ),(Case_Team__c  =&quot;Billing&quot; ),(Case_Team__c  =&quot;Saver Success&quot; ), (Case_Team__c  =&quot;Single(k)&quot;), (Case_Team__c  =&quot;Compliance&quot; )),  (ISBLANK( ParentId )),   NOT(ISBLANK(  Contact.Id  )),   NOT( CONTAINS( Contact.Email , &quot;myubiquity.com&quot;)),   (Contact.Survey_Opt_Out__c = false),  OR  ( ISBLANK(Contact.Last_Created_Survey_Date__c),  (TODAY () - (Contact.Last_Created_Survey_Date__c)) &gt; 60), BEGINS (OwnerId, &apos;005&apos;), DATEVALUE(ClosedDate) = TODAY ())</formula>
