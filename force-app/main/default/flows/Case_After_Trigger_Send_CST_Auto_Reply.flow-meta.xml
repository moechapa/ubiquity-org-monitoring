    <decisions>
        <name>Is_this_a_bounce_check</name>
        <label>Is this a bounce check</label>
        <locationX>644</locationX>
        <locationY>1055</locationY>
        <defaultConnector>
            <targetReference>Alert_to_auto_response_fail</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Bounce_Retry_Once</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Flow.FaultMessage</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>currently marked as bounced</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Contact.EmailBouncedDate</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>threeMonthsAgo</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Wipe_bounce_from_contact</leftValueReference>
                <operator>WasVisited</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Wipe_bounce_from_contact</targetReference>
            </connector>
            <label>Bounce - Retry Once</label>
        </rules>
