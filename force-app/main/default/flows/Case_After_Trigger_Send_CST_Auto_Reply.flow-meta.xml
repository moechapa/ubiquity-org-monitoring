    <description>Updated email body per Peggy&apos;s request.  Adding some additional formatting + links to help center topics.</description>
    <environments>Default</environments>
    <formulas>
        <name>last5Minutes</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} - (5/60/24)</expression>
    </formulas>
    <formulas>
        <name>recipientEmail</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.ContactEmail})||CONTAINS({!$Record.SuppliedEmail},&quot;noreply&quot;),{!$Record.SuppliedEmail},{!$Record.ContactEmail})</expression>
    </formulas>
    <formulas>
        <name>recipientName</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.ContactId}),{!$Record.SuppliedName},{!$Record.Contact.FirstName})</expression>
    </formulas>
    <formulas>
        <name>threeMonthsAgo</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.InterviewStartTime} - 90</expression>
    </formulas>
