    <formulas>
        <name>amountChanged</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Line_Item.ListPrice} - {!Get_Line_Item.Discount__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>discountAmount</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Line_Item.ListPrice} * {!Get_Line_Item.Discount_Percentage__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>discountPercentage</name>
        <dataType>Number</dataType>
        <expression>({!Get_Line_Item.Discount__c} / {!Get_Line_Item.ListPrice}) * 100</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>discountTotal</name>
        <dataType>Currency</dataType>
        <expression>IF (ISBLANK({!Get_Line_Item.Discount__c}), {!Get_Line_Item.ListPrice} - ({!Get_Line_Item.Discount_Percentage__c} * {!Get_Line_Item.ListPrice}), IF(NOT(ISBLANK({!Get_Line_Item.Discount__c}))&amp;&amp;NOT(ISBLANK({!Get_Line_Item.Discount__c})),{!Get_Line_Item.ListPrice} - {!Get_Line_Item.Discount__c}, 0))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>flatfeeCalculation</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Opp.Amount_Before_Discount__c}  * (1-{!Get_Opp.Discount_Percentage__c} )</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>hourlyAmount</name>
        <dataType>Currency</dataType>
        <expression>IF(

ISBLANK({!Get_Opp.Actual_Hours_Worked__c}),
 
(({!Get_Opp.Estimated_Hours_Needed__c} *{!Get_Opp.Rate__c}) )*
(1-{!Get_Opp.Discount_Percentage__c} ),

(({!Get_Opp.Actual_Hours_Worked__c} * {!Get_Opp.Rate__c} ) )*
(1-{!Get_Opp.Discount_Percentage__c} )
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>percentChanged</name>
        <dataType>Currency</dataType>
        <expression>{!Get_Line_Item.ListPrice} - ({!Get_Line_Item.Discount_Percentage__c} * {!Get_Line_Item.ListPrice})</expression>
        <scale>2</scale>
    </formulas>
