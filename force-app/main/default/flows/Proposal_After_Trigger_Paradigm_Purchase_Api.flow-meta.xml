        <name>autoDeferralType</name>
        <dataType>String</dataType>
        <expression>IF(
   {!$Record.Auto_Deferral_Type__c} = &quot;Flat&quot;, 
        &quot;FLAT&quot;,  
    IF(
        {!$Record.Auto_Deferral_Type__c} = &quot;Escalating&quot;, 
        &quot;ESCALATING&quot;, 
        null
    )
)</expression>
    </formulas>
    <formulas>
        <description>Will be used to populate Auto_Deferral_Type__c for internal OPD proposals.</description>
        <name>autoDeferralTypeInternal</name>
        <dataType>String</dataType>
        <expression>IF(
    ISBLANK({!$Record.Min_Auto_Deferral_Percentage__c}),
    NULL,
    IF(
        NOT(ISBLANK({!$Record.Max_Auto_Deferral_Percentage__c})),
        IF(
           {!$Record.Min_Auto_Deferral_Percentage__c} &lt;&gt;{!$Record.Max_Auto_Deferral_Percentage__c},
            &quot;ESCALATING&quot;,
            &quot;FLAT&quot;
        ),
        NULL
    )
)</expression>
    </formulas>
    <formulas>
        <name>SafeHarborMatchDeterminationPeriod</name>
        <dataType>String</dataType>
        <expression>IF(
    NOT(
        ISPICKVAL({!$Record.Safe_Harbor__c}, &quot;None&quot;) || 
        ISPICKVAL({!$Record.Safe_Harbor__c}, &quot;No&quot;) || 
        ISPICKVAL({!$Record.Safe_Harbor__c}, &quot;Yes&quot;)
    ),
    CASE(
        {!$Record.Safe_Harbor__c},
        &quot;3% Non-Elective Contribution (NEC)&quot;, &quot;Annual&quot;,
        &quot;3% QACA non-elective&quot;, &quot;Annual&quot;,
        &quot;Per Pay Period&quot; 
    ),
    NULL 
)</expression>
    </formulas>
    <formulas>
        <name>yearAutoDeferralIncreseInternal</name>
        <dataType>Number</dataType>
        <expression>IF(
    AND(
        NOT(ISBLANK({!$Record.Min_Auto_Deferral_Percentage__c})),
        {!$Record.Min_Auto_Deferral_Percentage__c} &lt;&gt; {!$Record.Max_Auto_Deferral_Percentage__c}
    ),
    1,
    NULL
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>yearlyAutoDeferralPercentageIncrease</name>
        <dataType>Number</dataType>
        <expression>IF(
    OR(
        ISBLANK({!$Record.Auto_Deferral_Type__c}), 
        {!$Record.Auto_Deferral_Type__c} = &quot;Flat&quot;
    ), 
    null, 
    1
)</expression>
        <scale>0</scale>
    </formulas>
