                <elementReference>$Record.HM_Billing_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.CSAT_Impact__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.HM_CSAT_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.NPS_Impact__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.HM_NPS_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Payroll_Impact__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.HM_Payroll_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Review_Impact__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.HM_Review_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Client_Health_Score__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Client_Health_Score__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Plan_Status__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Status__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Record_Event__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Monthly Update</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Plan__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Check_for_last_month_score</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Assign_percent_change</name>
        <label>Assign percent change</label>
        <locationX>50</locationX>
        <locationY>684</locationY>
        <assignmentItems>
            <assignToReference>newHealthScoreRecord.Percent_Change__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>percentDiff</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Create_HS_Record</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Check_for_last_month_score</name>
        <label>Check for last month score</label>
        <locationX>182</locationX>
        <locationY>576</locationY>
        <defaultConnector>
            <targetReference>Create_HS_Record</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>We_got_a_score</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>prevMonthScore</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_percent_change</targetReference>
            </connector>
            <label>We got a score</label>
        </rules>
    </decisions>
    <decisions>
        <name>First_of_the_month_check</name>
        <label>First of the month check</label>
        <locationX>336</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Generate_HM</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>firstOfMonth</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Grab_last_month_health_score</targetReference>
            </connector>
            <label>Generate HM</label>
        </rules>
    </decisions>
    <description>One time run~</description>
    <environments>Default</environments>
    <formulas>
        <name>firstOfMonth</name>
        <dataType>Boolean</dataType>
        <expression>DAY(TODAY()) = 1</expression>
    </formulas>
    <formulas>
        <description>The first of last month, for grabbing prev health score</description>
        <name>lastMonth1st</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS(TODAY(),-1)</expression>
    </formulas>
    <formulas>
        <name>percentDiff</name>
        <dataType>Number</dataType>
        <expression>(({!newHealthScoreRecord.Client_Health_Score__c} / {!prevMonthScore}) - 1) * 100</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>Plan | Scheduled | Create Monthly Health Score {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Plan | Scheduled | Create Monthly Health Score</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Create_HS_Record</name>
        <label>Create HS Record</label>
        <locationX>182</locationX>
        <locationY>876</locationY>
        <inputReference>newHealthScoreRecord</inputReference>
    </recordCreates>
    <recordLookups>
        <name>Grab_last_month_health_score</name>
        <label>Grab last month health score</label>
        <locationX>182</locationX>
        <locationY>360</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Assign_Health_Score_Values</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Plan__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>CreatedDate</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>lastMonth1st</elementReference>
            </value>
        </filters>
        <filters>
            <field>CreatedDate</field>
            <operator>LessThan</operator>
            <value>
                <elementReference>$Flow.CurrentDate</elementReference>
            </value>
        </filters>
        <filters>
            <field>Record_Event__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Monthly Update</stringValue>
            </value>
        </filters>
        <object>Client_Health_Score__c</object>
        <outputAssignments>
            <assignToReference>prevMonthScore</assignToReference>
            <field>Client_Health_Score__c</field>
        </outputAssignments>
        <sortField>CreatedDate</sortField>
        <sortOrder>Desc</sortOrder>
    </recordLookups>
    <start>
        <locationX>210</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>First_of_the_month_check</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Status__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Inactive</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>01237000000TgnVAAS</stringValue>
            </value>
        </filters>
        <object>Plan__c</object>
        <schedule>
            <frequency>Once</frequency>
            <startDate>2024-11-01</startDate>
            <startTime>08:00:00.000Z</startTime>
        </schedule>
        <triggerType>Scheduled</triggerType>
    </start>
    <status>Obsolete</status>
    <variables>
        <name>newHealthScoreRecord</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Client_Health_Score__c</objectType>
    </variables>
    <variables>
        <name>prevMonthScore</name>
        <dataType>Number</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <scale>0</scale>
    </variables>
