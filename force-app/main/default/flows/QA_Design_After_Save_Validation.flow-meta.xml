                </rightValue>
            </conditions>
            <connector>
                <targetReference>Find_related_but_incomplete_qa_forms</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Newly Deactivated</label>
        </rules>
