<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>65.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Add_to_Collection</name>
        <label>Add to Collection</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>casesToCreate</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>caseToWork</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_Through_Owners</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Assign_Case_Values</name>
        <label>Assign Case Values</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>caseToWork.AccountId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Plan__r.Account__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseToWork.Plan__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Plan__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseToWork.Sender_Role__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Sponsor</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseToWork.Origin</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Internal</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseToWork.Subject</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Plan Amendment Complete, Please Review</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseToWork.Description</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>caseDesc</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>What_Cases_to_Create</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Assign_Owner</name>
        <label>Assign Owner</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>caseToWork.OwnerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Loop_Through_Owners</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Add_to_Collection</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Assign_Payroll_as_Owner</name>
        <label>Assign Payroll as Owner</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>caseToWork.OwnerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>payroll</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Create_Case_for_Payroll</targetReference>
        </connector>
    </assignments>
    <assignments>
        <description>Payroll
A&amp;F
RW</description>
        <name>Who_Needs_Cases</name>
        <label>Who Needs Cases</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>caseOwners</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>accountingFinance</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseOwners</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>payroll</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>caseOwners</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>rW</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_Through_Owners</targetReference>
        </connector>
    </assignments>
    <constants>
        <name>accountingFinance</name>
        <dataType>String</dataType>
        <value>
            <stringValue>00500000006yEak</stringValue>
        </value>
    </constants>
    <constants>
        <name>payroll</name>
        <dataType>String</dataType>
        <value>
            <stringValue>00537000002STLD</stringValue>
        </value>
    </constants>
    <constants>
        <name>rW</name>
        <dataType>String</dataType>
        <value>
            <stringValue>005Ho00000AnvFJ</stringValue>
        </value>
    </constants>
    <decisions>
        <name>What_Cases_to_Create</name>
        <label>What Cases to Create</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Category_Plan_Source_Update</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Category__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Plan Source Update</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Payroll_as_Owner</targetReference>
            </connector>
            <label>Category = Plan Source Update</label>
        </rules>
        <rules>
            <name>Category_Plan_Information_Update</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Category__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Plan Information Update</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Who_Needs_Cases</targetReference>
            </connector>
            <label>Category = Plan Information Update</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>recordURL</name>
        <dataType>String</dataType>
        <expression>LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_260)) &amp; {!$Record.Id}</expression>
    </formulas>
    <interviewLabel>Plan Amendment | After Update | Create Cases {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Plan Amendment | After Update | Create Cases</label>
    <loops>
        <name>Loop_Through_Owners</name>
        <label>Loop Through Owners</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <collectionReference>caseOwners</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_Owner</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Create_Cases</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Create_Case_for_Payroll</name>
        <label>Create Case for Payroll</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputReference>caseToWork</inputReference>
    </recordCreates>
    <recordCreates>
        <name>Create_Cases</name>
        <label>Create Cases</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputReference>casesToCreate</inputReference>
    </recordCreates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Assign_Case_Values</targetReference>
        </connector>
        <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Amendment_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Amendment Completed</stringValue>
            </value>
        </filters>
        <object>Plan_Amendment__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <textTemplates>
        <name>caseDesc</name>
        <isViewedAsPlainText>true</isViewedAsPlainText>
        <text>Please review {!recordURL} to determine if any updates are required for your team. If updates are required, please make the updates and close out this case</text>
    </textTemplates>
    <variables>
        <name>caseOwners</name>
        <dataType>String</dataType>
        <isCollection>true</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>casesToCreate</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Case</objectType>
    </variables>
    <variables>
        <name>caseToWork</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Case</objectType>
    </variables>
</Flow>
