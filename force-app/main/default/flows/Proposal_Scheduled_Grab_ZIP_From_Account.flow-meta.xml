        <isOutput>false</isOutput>
    </variables>
    <waits>
        <name>Loop_Pause</name>
        <label>Loop Pause</label>
        <locationX>270</locationX>
        <locationY>1572</locationY>
        <defaultConnectorLabel>Default Path</defaultConnectorLabel>
        <waitEvents>
            <name>BriefPause</name>
            <conditionLogic>and</conditionLogic>
            <connector>
                <targetReference>Set_Loop_to_0_0</targetReference>
            </connector>
            <eventType>AlarmEvent</eventType>
            <inputParameters>
                <name>AlarmTime</name>
                <value>
                    <elementReference>NowMinus59Mins</elementReference>
                </value>
            </inputParameters>
            <inputParameters>
                <name>TimeOffset</name>
                <value>
                    <numberValue>1.0</numberValue>
                </value>
            </inputParameters>
            <inputParameters>
                <name>TimeOffsetUnit</name>
                <value>
                    <stringValue>Hours</stringValue>
                </value>
            </inputParameters>
            <label>BriefPause</label>
        </waitEvents>
    </waits>
</Flow>
