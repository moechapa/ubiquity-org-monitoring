<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>CensusCompletePercentageOver1</name>
        <label>CensusCompletePercentageOver1</label>
        <locationX>182</locationX>
        <locationY>647</locationY>
        <defaultConnectorLabel>No/Already Recorded</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CensusCompletePercentage</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Census_and_Eligibility_Uploaded__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Implementation_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>No Start</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Implementation_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Census_and_Eligibility_Date</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Kotapay_Completed</name>
        <label>Kotapay Completed?</label>
        <locationX>182</locationX>
        <locationY>311</locationY>
        <defaultConnector>
            <targetReference>CensusCompletePercentageOver1</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Complete</defaultConnectorLabel>
        <rules>
            <name>Kotapay_Complete_But_Box_Already_Checked</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Kotapay_Complete__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ACH_Application__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CensusCompletePercentageOver1</targetReference>
            </connector>
            <label>Kotapay Complete But Box Already Checked</label>
        </rules>
        <rules>
            <name>Kotapay_Complete</name>
            <conditionLogic>1 AND 2 AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Kotapay_Complete__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ACH_Application__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Custodian__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Schwab &amp; Co., Inc.</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Custodian__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Schwab Trust</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_ACH_Box</targetReference>
            </connector>
            <label>Kotapay Complete</label>
        </rules>
    </decisions>
    <description>When Count of Employees changes, check Census Eligibility Percentage, if greater or equal to 1, stamp Census and Eligibility Uploaded. Added check for Schwab in kotapay decision 12-1-22</description>
    <environments>Default</environments>
    <formulas>
        <name>CensusCompletePercentage</name>
        <dataType>Number</dataType>
        <expression>{!$Record.Count_of_Employees_Census__c}/{!$Record.Account__r.NumberOfEmployees}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Today</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>
    <interviewLabel>Implementation | Before Trigger | Update Census {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Implementation | Before Trigger | Update Census and Eligibility Upload Date</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Check_ACH_Box</name>
        <label>Check ACH Box</label>
        <locationX>138</locationX>
        <locationY>431</locationY>
        <connector>
            <targetReference>CensusCompletePercentageOver1</targetReference>
        </connector>
        <inputAssignments>
            <field>ACH_Application__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Census_and_Eligibility_Date</name>
        <label>Update Census and Eligibility Date</label>
        <locationX>50</locationX>
        <locationY>767</locationY>
        <inputAssignments>
            <field>Census_and_Eligibility_Uploaded__c</field>
            <value>
                <elementReference>Today</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>56</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Kotapay_Completed</targetReference>
        </connector>
        <object>Implementation__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
