<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>53.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Docusign_Envelope_Status</name>
        <label>Docusign Envelope Status</label>
        <locationX>168</locationX>
        <locationY>368</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Docusign_Sent</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.dsfs__Envelope_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sent</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.dsfs__Opportunity__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.dsfs__Opportunity__r.Type</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Ancillary Billing</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Opp_Stage_Pending_Signature</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>Docusign Sent</label>
        </rules>
        <rules>
            <name>Docusign_Complete</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.dsfs__Envelope_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.dsfs__Opportunity__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.dsfs__Envelope_Status__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.dsfs__Opportunity__r.Type</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Ancillary Billing</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Opp_Stage_Docusign_Complete</targetReference>
            </connector>
            <label>Docusign Complete</label>
        </rules>
    </decisions>
    <interviewLabel>Docusign Status | After Trigger | Ancillary Billing Opportunity Updates {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Docusign Status | After Trigger | Ancillary Billing Opportunity Updates</label>
    <processMetadataValues>
