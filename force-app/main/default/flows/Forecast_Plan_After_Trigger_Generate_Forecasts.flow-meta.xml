    </recordLookups>
    <start>
        <locationX>590</locationX>
        <locationY>48</locationY>
        <connector>
            <targetReference>Validate_Dates</targetReference>
        </connector>
        <filterFormula>(ISCHANGED({!$Record.Forecast_Start_Date__c}) || ISCHANGED({!$Record.Forecast_End_Date__c}) || ISNEW())
&amp;&amp;
NOT(ISNULL({!$Record.Forecast_Start_Date__c})) &amp;&amp; NOT(ISNULL({!$Record.Forecast_End_Date__c}))</filterFormula>
        <object>Forecast_Plan__c</object>
