        </rules>
    </decisions>
    <decisions>
        <description>Checking to verify that the first date of the plan is the first of a month AND the last day of the plan is the last day of that month.</description>
        <name>Validate_Dates</name>
        <label>Validate Dates</label>
        <locationX>716</locationX>
        <locationY>383</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Dates_Valid</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>DateVar</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>MonthLastDay</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>RecordStartDay</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Existing_Forecasts</targetReference>
            </connector>
            <label>Dates Valid</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>DateVarMinusMonth</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS({!DateVar},-1)</expression>
    </formulas>
    <formulas>
        <name>ForecastNameCalc</name>
        <dataType>String</dataType>
        <expression>CASE(
MONTH( {!DateVar} ),
1, &quot;January&quot;,
2, &quot;February&quot;,
3, &quot;March&quot;,
4, &quot;April&quot;,
5, &quot;May&quot;,
6, &quot;June&quot;,
7, &quot;July&quot;,
8, &quot;August&quot;,
9, &quot;September&quot;,
10, &quot;October&quot;,
11, &quot;November&quot;,
&quot;December&quot;
) + &quot; &quot; +{!$Record.Name}</expression>
    </formulas>
    <formulas>
        <name>MonthFirstDay</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!DateVar}),MONTH({!DateVar}),1)</expression>
    </formulas>
    <formulas>
        <name>MonthLastDay</name>
        <dataType>Date</dataType>
        <expression>IF( MONTH( {!DateVar} ) = 12, 
DATE( YEAR( {!DateVar} ), 12, 31 ), 
DATE( YEAR( {!DateVar} ), MONTH( {!DateVar} ) + 1, 1) - 1)</expression>
    </formulas>
    <formulas>
        <name>NumofMonths</name>
        <dataType>Number</dataType>
        <expression>((YEAR({!$Record.Forecast_End_Date__c}) - YEAR({!$Record.Forecast_Start_Date__c})) * 12)
+ (MONTH({!$Record.Forecast_End_Date__c}) - MONTH({!$Record.Forecast_Start_Date__c}))
+ 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>RecordStartDay</name>
        <dataType>Number</dataType>
        <expression>DAY({!$Record.Forecast_Start_Date__c})</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Forecast Plan {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Forecast Plan | After Trigger | Generate Forecasts</label>
