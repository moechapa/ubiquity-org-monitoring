        <locationX>1380</locationX>
        <locationY>357</locationY>
        <defaultConnector>
            <targetReference>Comparative_Loop</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
