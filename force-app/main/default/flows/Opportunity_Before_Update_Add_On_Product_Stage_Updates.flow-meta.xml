                </rightValue>
            </conditions>
            <connector>
                <targetReference>Define_401k_id</targetReference>
            </connector>
            <label>I am 401k</label>
        </rules>
        <rules>
            <name>I_am_EPS</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Record_Type_Loop.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Enhanced_Payroll_Services_Product</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Define_EPS_Id</targetReference>
            </connector>
            <label>I am EPS</label>
        </rules>
    </decisions>
