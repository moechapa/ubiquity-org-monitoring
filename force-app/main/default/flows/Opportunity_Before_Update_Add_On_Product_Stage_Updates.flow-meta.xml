            </connector>
            <label>I am ancillary flat</label>
        </rules>
        <rules>
            <name>I_am_ancillary_hourly</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Record_Type_Loop.DeveloperName</leftValueReference>
