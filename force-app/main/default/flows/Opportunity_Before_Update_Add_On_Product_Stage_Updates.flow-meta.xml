    <decisions>
        <description>Is Enhanced Payroll Services Product and Introduction email sent?</description>
        <name>Introduction_Email_Sent</name>
        <label>Intro Email Sent?</label>
        <locationX>710</locationX>
        <locationY>1103</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record__Prior.Introduction_Email_Sent__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Introduction_Email_Sent__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>epsId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Stage1</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Is product type = 401(k) and Record Type not equal to Ancillary Billing Flat Fee or Ancillary Billing Hourly Estimate</description>
        <name>Product_Type_401_k</name>
        <label>Product Type = 401(k) ?</label>
        <locationX>281</locationX>
        <locationY>887</locationY>
        <defaultConnector>
            <targetReference>Product_Type_Single_k</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes401k</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Product_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>401(k)</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordTypeId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>ancillaryFlatId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordTypeId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>ancillaryHourlyId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_401_k_Record_Type</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Is product type = Single(k)</description>
