    <description>Flow replaces 3 PBs that deal with ancillary billing opps being created or edited.</description>
    <environments>Default</environments>
    <formulas>
        <name>actualAmountBeforeDiscount</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.Rate__c} * {!$Record.Actual_Hours_Worked__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>actualHourlyAmount</name>
        <dataType>Currency</dataType>
        <expression>{!actualAmountBeforeDiscount} * (1-{!$Record.Discount_Percentage__c}/100)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ancBillingAmount</name>
        <dataType>Currency</dataType>
        <expression>({!$Record.Actual_Hours_Worked__c} * {!$Record.Rate__c}) - {!$Record.Discount__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>closeDate</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 14</expression>
    </formulas>
    <formulas>
        <name>flatAmount</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.Amount_Before_Discount__c} * (1-({!$Record.Discount_Percentage__c}/100))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>hourlyAmount</name>
        <dataType>Currency</dataType>
        <expression>({!$Record.Rate__c} * {!$Record.Estimated_Hours_Needed__c}) * (1 - ({!$Record.Discount_Percentage__c}/100))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>hourlyAmountBeforeDiscount</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.Rate__c} * {!$Record.Estimated_Hours_Needed__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>isNew</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()</expression>
    </formulas>
