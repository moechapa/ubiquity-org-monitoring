        <dataType>String</dataType>
        <expression>&quot;A Transition record has been created for &quot; + {!$Record.Plan__r.Name} + &quot;, please begin the process to terminate payroll services&quot;</expression>
    </formulas>
    <formulas>
        <name>caseSubject</name>
        <dataType>String</dataType>
