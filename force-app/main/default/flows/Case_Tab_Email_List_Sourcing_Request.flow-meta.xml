    <formulas>
        <name>ChildCaseDescription</name>
        <dataType>String</dataType>
        <expression>&quot;Please review and approve the following request, and any subsequent prospect list delivered:&quot; + BR() +
BR() +
&quot;Requestor: &quot; + {!Requestor} + BR() +
&quot;Ubiquity Leader: &quot; + TEXT({!UbiquityLeader}) + BR() +
