            </connector>
            <label>Expired</label>
        </rules>
        <rules>
            <name>Purchase</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>purchase</stringValue>
                </rightValue>
            </conditions>
            <conditions>
