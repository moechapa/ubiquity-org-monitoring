        <defaultConnectorLabel>Purchased and Closed Won</defaultConnectorLabel>
        <rules>
            <name>Purchased_and_Committed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Purchased</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Principal_Opportunity_Stage__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Closed Won</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>New_Purchase</targetReference>
            </connector>
            <label>Purchased and Committed</label>
        </rules>
        <rules>
            <name>Submitted</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Purchased</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Pending_Purchase</targetReference>
            </connector>
            <label>Submitted</label>
        </rules>
    </decisions>
