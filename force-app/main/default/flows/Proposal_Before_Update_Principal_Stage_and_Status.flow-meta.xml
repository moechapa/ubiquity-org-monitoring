            </connector>
            <label>Purchase</label>
        </rules>
        <rules>
            <name>SaveShare</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>share</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>save</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_SaveShare_Values</targetReference>
            </connector>
            <label>SaveShare</label>
        </rules>
        <rules>
            <name>ReviewNewInfo</name>
            <conditionLogic>(1 OR 2 OR 4) AND (3 OR 5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>review</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>new</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>IsBlank</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Stage_Flag__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>start</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pricing</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Initial_Values</targetReference>
            </connector>
            <label>ReviewNewInfo</label>
        </rules>
