    <environments>Default</environments>
    <formulas>
        <description>Some transitions do not get docusigns, this web lets you know they should not</description>
        <name>dontSendDocusign</name>
        <dataType>Boolean</dataType>
        <expression>((ISPICKVAL({!$Record.Transition_Type__c}, &quot;Deconversion&quot;) || ISPICKVAL({!$Record.Transition_Type__c}, &quot;Termination/Merger&quot;)) &amp;&amp; ISPICKVAL({!$Record.Service_Option__c}, &quot;RK Only&quot;))
||
(ISPICKVAL({!$Record.Transition_Type__c}, &quot;Deconversion&quot;) &amp;&amp; ({!fullServiceNonSimplyMatrix} = TRUE || {!fullServiceSchwabBD} = TRUE))</expression>
    </formulas>
    <formulas>
        <description>Full service plans that are not simply where the custodian is matrix</description>
        <name>fullServiceNonSimplyMatrix</name>
        <dataType>Boolean</dataType>
        <expression>ISPICKVAL({!$Record.Custodian__c}, &quot;MG Trust Company, LLC.&quot;) &amp;&amp; NOT(ISPICKVAL({!$Record.Plan__r.Business_Unit__c}, &quot;Simply Retirement&quot;)) &amp;&amp; (ISPICKVAL({!$Record.Plan__r.Product_Type1__c}, &quot;401(k)&quot;) || ISPICKVAL({!$Record.Plan__r.Product_Type1__c}, &quot;PEP&quot;))</expression>
    </formulas>
    <formulas>
        <name>fullServiceSchwabBD</name>
        <dataType>Boolean</dataType>
        <expression>ISPICKVAL({!$Record.Custodian__c}, &quot;Schwab &amp; Co., Inc.&quot;) &amp;&amp; (ISPICKVAL({!$Record.Plan__r.Product_Type1__c}, &quot;401(k)&quot;) || ISPICKVAL({!$Record.Plan__r.Product_Type1__c}, &quot;PEP&quot;))</expression>
    </formulas>
    <formulas>
        <name>isLiquidationDateToday</name>
        <dataType>Boolean</dataType>
