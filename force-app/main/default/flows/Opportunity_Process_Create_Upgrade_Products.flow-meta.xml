    <description>Changed &quot;Service Option&quot; in get elements to account for Simply products</description>
    <environments>Default</environments>
    <formulas>
        <name>EmployeesNew</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Get_Opp.Service_Option__c},&quot;Simply Retirement Unbundled&quot;), &quot;1+&quot;,
IF(ISPICKVAL({!Get_Opp.Service_Option__c},&quot;Simply Retirement Bundled&quot;), &quot;1+&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 15, &quot;2-15&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 30, &quot;16-30&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 50, &quot;31-50&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 75, &quot;51-75&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 100, &quot;76-100&quot;,
&quot;101+&quot;)))))))</expression>
    </formulas>
    <formulas>
        <name>EmployeesOld</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!Get_Opp.Previous_Service_Option__c},&quot;Simply Retirement Unbundled&quot;), &quot;1+&quot;,
IF(ISPICKVAL({!Get_Opp.Previous_Service_Option__c},&quot;Simply Retirement Bundled&quot;), &quot;1+&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 15, &quot;2-15&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 30, &quot;16-30&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 50, &quot;31-50&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 75, &quot;51-75&quot;,
IF({!Get_Opp.Number_of_Employees__c} &lt;= 100, &quot;76-100&quot;,
&quot;101+&quot;)))))))</expression>
    </formulas>
    <formulas>
        <name>FormattedPreviousServiceOption</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_Opp.Previous_Service_Option__c})=&quot;Simply Retirement Unbundled&quot;, &quot;Simply TPA&quot;,
IF(TEXT({!Get_Opp.Previous_Service_Option__c})=&quot;Simply Retirement Bundled&quot;, &quot;Simply |&quot;,
TEXT({!Get_Opp.Previous_Service_Option__c})
))</expression>
    </formulas>
    <formulas>
        <name>FormattedServiceOption</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!Get_Opp.Service_Option__c})=&quot;Simply Retirement Unbundled&quot;, &quot;Simply TPA&quot;,
IF(TEXT({!Get_Opp.Service_Option__c})=&quot;Simply Retirement Bundled&quot;, &quot;Simply |&quot;,
TEXT({!Get_Opp.Service_Option__c})
))</expression>
    </formulas>
    <formulas>
        <name>NewComp</name>
        <dataType>String</dataType>
        <expression>&quot;New Comp&quot;</expression>
    </formulas>
    <formulas>
        <name>prevDiscount</name>
        <dataType>Number</dataType>
        <expression>{!Get_Price_Book_Entry.UnitPrice} * 2</expression>
        <scale>2</scale>
    </formulas>
