            </value>
        </assignmentItems>
        <connector>
            <targetReference>Reset_Counter</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Increment_noWorkCapacityAgentCount</name>
        <label>Increment noWorkCapacityAgentCount</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>noWorkCapacityAgentCount</assignToReference>
