        </connector>
    </assignments>
    <assignments>
        <name>Reset_Counter</name>
        <label>Reset Counter</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>agentWorkItemCount</assignToReference>
            <operator>Assign</operator>
            <value>
                <numberValue>0.0</numberValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_through_Members</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Are_any_agents_available_for_chat</name>
        <label>Are any agents available for chat?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Flip_Agent_Availability_Flag_to_False</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_available</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>hasWorkCapacityAgentCount</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Flip_Agent_Availability_Flag_to_Tue</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Messaging_Session_Found</name>
        <label>Messaging Session Found?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Found1</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Messaging_Session.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Which_Channel</targetReference>
            </connector>
            <label>Found</label>
        </rules>
    </decisions>
    <decisions>
        <name>Online_Agent_Found</name>
        <label>Online Agent Found?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Reset_Counter</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Online_Agents</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Agent_Work</targetReference>
            </connector>
            <label>Found</label>
        </rules>
    </decisions>
    <decisions>
        <name>Which_Channel</name>
        <label>Which Channel</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Omni_Support</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Messaging_Session.ChannelName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Omni Messaging</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Omni_Support_Queue_Id</targetReference>
            </connector>
            <label>Omni Support</label>
        </rules>
        <rules>
            <name>Login_Help</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Messaging_Session.ChannelName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Login Help</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Omni_Login_Help_Queue_Id</targetReference>
            </connector>
            <label>Login Help</label>
        </rules>
    </decisions>
    <decisions>
        <name>Work_Capacity_at_Max</name>
        <label>Work Capacity at Max?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Increment_hasWorkCapacityAgentCount</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>agentWorkItemCount</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Increment_noWorkCapacityAgentCount</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <interviewLabel>Messaging | Process | Get available Agents for Chat {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Messaging | Process | Check Agent Availability</label>
    <loops>
        <name>Loop_through_Members</name>
        <label>Loop through Members</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <collectionReference>Get_Queue_Members</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Get_Online_Agents</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Are_any_agents_available_for_chat</targetReference>
        </noMoreValuesConnector>
