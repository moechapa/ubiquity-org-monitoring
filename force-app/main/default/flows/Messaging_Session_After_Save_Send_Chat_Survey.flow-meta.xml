        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Contact_Eligible</name>
            <conditionLogic>1 AND 2 AND 3 AND NOT (4 OR 5)</conditionLogic>
            <conditions>
                <leftValueReference>messagingContact.Id</leftValueReference>
