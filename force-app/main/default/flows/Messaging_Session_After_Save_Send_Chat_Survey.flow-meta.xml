                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>messagingContact.Survey_Opt_Out__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
