                <operator>Contains</operator>
                <rightValue>
                    <stringValue>@myubiquity</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>messagingContact.Email</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>@principal</stringValue>
                </rightValue>
            </conditions>
            <connector>
