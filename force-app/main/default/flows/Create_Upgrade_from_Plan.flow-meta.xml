    <decisions>
        <name>Case_for_Onboarding</name>
        <label>Case for Onboarding</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Upgrade_Successfully_Created</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Bundled_SK_SK_to_Full_Service</name>
            <conditionLogic>1 OR 2 OR ((3 OR 4 OR 5) AND 6)</conditionLogic>
            <conditions>
                <leftValueReference>Service_Option_Screen</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Simply_Retirement_Bundled</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Service_Option_Screen</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>SingleKPlus</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Service_Option_Screen</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>CustomK</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Service_Option_Screen</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>ReserveK</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Service_Option_Screen</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>SaverK</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Plan.Product_Type1__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Single(k)</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Case_for_Onboarding</targetReference>
            </connector>
            <label>Bundled, SK+, SK to Full Service</label>
        </rules>
    </decisions>
