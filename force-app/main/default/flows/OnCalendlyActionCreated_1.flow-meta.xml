<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Advisor_Onboarding_Event_Check</name>
        <label>Advisor Onboarding Event Check</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>General_Create_Event_Flow</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Advisor_Onboarding_Event_Created</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>advisor_onboarding</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>myRule_9_A1</targetReference>
            </connector>
            <label>Advisor Onboarding Event Created</label>
        </rules>
    </decisions>
    <decisions>
        <name>Cancel_Check</name>
        <label>Cancel Check</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Onboarding_Event_Check</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Invitee_Canceled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>invitee.canceled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>myRule_1_A1</targetReference>
            </connector>
            <label>Invitee Canceled</label>
        </rules>
    </decisions>
    <decisions>
        <name>Onboarding_Event_Check</name>
        <label>Onboarding Event Check</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Advisor_Onboarding_Event_Check</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Onboarding_Event_Created</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>introductory-call-1-of-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>add-your-employees-call-2-of-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>final-review-and-funding</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>introductory-call-401-k-plan-setup-call-1-of-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>15min</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>plan-setup-call-2-of-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>plan-setup-call-3-of-3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.EventTypeSlug__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>plan-setup-call-1-of-3</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>myRule_3_A1</targetReference>
            </connector>
            <label>Onboarding Event Created</label>
        </rules>
    </decisions>
    <description>Migrated from the OnCalendlyActionCreated process using multiple criteria.
When Calendly Creates a Record in Salesforce, run certain flows.</description>
    <environments>Default</environments>
