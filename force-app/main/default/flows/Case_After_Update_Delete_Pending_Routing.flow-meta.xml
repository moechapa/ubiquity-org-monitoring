<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Check_if_Pending_Routing_Found</name>
        <label>Check if Pending Routing Found</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Pending_Routing_Spotted</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_User_Locked_Routing</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Kill</targetReference>
            </connector>
            <label>Pending Routing Spotted</label>
        </rules>
    </decisions>
    <description>Deletes any PendingServiceRouting records created by &quot;Register Case In Omni&quot; flow when owner is changed.  Useful for when an inbound phone call case is created, then transferred to another user or non omni (slow, bad) queue.</description>
    <environments>Default</environments>
    <interviewLabel>Case | After Update | Delete Pending Routing {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Case | After Update | Delete Pending Routing</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordDeletes>
        <name>Kill</name>
        <label>Kill</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputReference>Get_User_Locked_Routing</inputReference>
    </recordDeletes>
    <recordLookups>
        <name>Get_User_Locked_Routing</name>
        <label>Get User Locked Routing</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Check_if_Pending_Routing_Found</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>PreferredUserId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record__Prior.OwnerId</elementReference>
            </value>
        </filters>
        <filters>
            <field>IsPreferredUserRequired</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>WorkItemId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>false</getFirstRecordOnly>
        <object>PendingServiceRouting</object>
        <queriedFields>Id</queriedFields>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_User_Locked_Routing</targetReference>
        </connector>
        <filterFormula>ISCHANGED({!$Record.OwnerId}) &amp;&amp; {!$Record.CreatedById} = {!$Record__Prior.OwnerId}</filterFormula>
        <object>Case</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
