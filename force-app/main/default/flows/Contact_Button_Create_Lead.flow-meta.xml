        <locationX>336</locationX>
        <locationY>350</locationY>
        <defaultConnector>
            <targetReference>Owner_Not_Set</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Yes</defaultConnectorLabel>
        <rules>
            <name>NoCorrect</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_Advisor_Contact.OwnerId</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>$User.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$UserRole.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>SDR</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Default_Lead_Source_Info</targetReference>
            </connector>
            <label>No</label>
        </rules>
    </decisions>
