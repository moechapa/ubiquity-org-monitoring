    <start>
        <locationX>452</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Does_TPA_EIN_need_correction</targetReference>
        </connector>
        <filterFormula>NOT(ISBLANK({!$Record.Opportunity__c}))
||
(NOT(ISBLANK({!$Record.Advisor_Contact__c})) &amp;&amp; ISBLANK({!$Record.Advisor_NPN__c}))
||
(NOT(ISNUMBER({!$Record.TPA_EIN__c})) &amp;&amp; NOT(ISBLANK({!$Record.TPA_EIN__c})))</filterFormula>
        <object>Proposal__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
